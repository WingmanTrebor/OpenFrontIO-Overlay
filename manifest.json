{
  "manifest_version": 3,
  "name": "OpenFrontIO Overlay",
  "version": "0.1",
  "description": "Adds an overlay to the OpenFrontIO game UI.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "OpenFrontIO Overlay"
  },
  "content_scripts": [
    {
      "matches": ["https://openfront.io/*", "http://openfront.io/*"],
      "js": ["contentScript.js"],
      "css": ["styles.css"]
    }
  ]
}